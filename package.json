{
  "name": "occams-conf",
  "version": "2.0.0",
  "description": "A simple config module",
  "license": "ISC",
  "repository": "lxghtless/occams-conf",
  "author": {
    "name": "lxghtlxss",
    "email": "lxghtl3ss@gmail.com",
    "url": "github.com/lxghtless"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "xo && nyc ava",
    "test-sys": "ava 'sys-test/**/test.js' 'sys-test/**/*.test.js'",
    "test-all": "ava 'sys-test/**/test.js' 'sys-test/**/*.test.js' 'test/**/test.js' 'test/**/*.test.js'",
    "test-sys-with-coverage": "nyc ava 'sys-test/**/test.js' 'sys-test/**/*.test.js'",
    "test-all-with-coverage": "nyc ava 'sys-test/**/test.js' 'sys-test/**/*.test.js' 'test/**/test.js' 'test/**/*.test.js'"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "config"
  ],
  "dependencies": {
    "delay": "^4.2.0",
    "p-forever": "^2.1.0"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "nyc": "^14.1.0",
    "xo": "^0.24.0"
  },
  "xo": {
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ],
      "camelcase": 0,
      "capitalized-comments": 0,
      "ava/no-ignored-test-files": 0
    }
  },
  "ava": {
    "sources": [
      "index.js"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "verbose": true
  },
  "nyc": {
    "all": true,
    "include": [
      "index.js"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "occams-conf": {
    "name": "config.js"
  }
}
