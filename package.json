{
  "name": "occams-conf",
  "version": "3.0.0-1",
  "description": "A simple config module",
  "license": "ISC",
  "repository": "lxghtless/occams-conf",
  "author": {
    "name": "lxghtlxss",
    "email": "lxghtl3ss@gmail.com",
    "url": "github.com/lxghtless"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "node -r dotenv/config src/index.js",
    "test": "xo && nyc ava"
  },
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "keywords": [
    "config"
  ],
  "dependencies": {
    "find-up": "^4.1.0",
    "is-installed-globally": "^0.3.1",
    "is-url-superb": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "dotenv": "^8.2.0",
    "nyc": "^14.1.1",
    "xo": "^0.25.3"
  },
  "xo": {
    "rules": {
      "capitalized-comments": 0
    }
  },
  "ava": {
    "sources": [
      "index.js"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "verbose": true,
    "environmentVariables": {
      "OCCAMS_CONF_MANUAL_INIT": "true"
    }
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "occams-conf": {
    "path": null,
    "name": "config.js"
  }
}
